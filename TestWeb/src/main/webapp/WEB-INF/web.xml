<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>Archetype Created Web Application</display-name>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>TestServ</servlet-name>
		<servlet-class>com.example.web.TestServ</servlet-class>
	</servlet>
	<servlet>
		<display-name>NoInterfaceViewServlet</display-name>
		<servlet-name>NoInterfaceViewServlet</servlet-name>
		<servlet-class>com.example.web.NoInterfaceViewServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>TestServ</servlet-name>
		<url-pattern>/serv</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>NoInterfaceViewServlet</servlet-name>
		<url-pattern>/NoInterfaceViewServlet</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>BeanLifeCycleServlet</servlet-name>
		<servlet-class>com.example.web.BeanLifeCycleServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>LoginServlet</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.example.web.loginapp.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>LogoutServlet</display-name>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.example.web.loginapp.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>PersonalDetailsServlet</display-name>
		<servlet-name>PersonalDetailsServlet</servlet-name>
		<servlet-class>com.example.web.loginapp.PersonalDetailsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>BeanLifeCycleServlet</servlet-name>
		<url-pattern>/BeanLifeCycleServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PersonalDetailsServlet</servlet-name>
		<url-pattern>/PersonalDetailsServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>SingletonTestServlet</servlet-name>
		<servlet-class>com.example.web.singleton.SingletonTestServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>AsynchronousdemoServlet</display-name>
		<servlet-name>AsynchronousdemoServlet</servlet-name>
		<servlet-class>com.example.web.asynchronous.AsynchronousdemoServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>AsynchronousSynchronousMixDemoServlet</display-name>
		<servlet-name>AsynchronousSynchronousMixDemoServlet</servlet-name>
		<servlet-class>com.example.web.asynchronous.AsynchronousSynchronousMixDemoServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>SingletonTestServlet</servlet-name>
		<url-pattern>/SingletonTestServlets</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsynchronousdemoServlet</servlet-name>
		<url-pattern>/AsynchronousdemoServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsynchronousSynchronousMixDemoServlet</servlet-name>
		<url-pattern>/AsynchronousSynchronousMixDemoServlet</url-pattern>
	</servlet-mapping>
</web-app>
